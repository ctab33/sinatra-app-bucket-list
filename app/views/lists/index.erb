<h3>Welcome, <%= current_user.username%>!</h3>

<% @lists.each do |list| %>
  <h3>These are Your Bucket Lists</h3>
  <ul class="list">
    <li><a href="/lists/<%=list.id%>" class="link_color"><%= list.name %></a></li>
  </ul>
  <div>
    <a href="/lists/new">Create A New List</a>
    <a href="/lists/<%=list.id%>/edit">Edit Your List</a>
    <a href="/goals/new">Add a Goal to Your List</a>
  </div>
  <% end %>
